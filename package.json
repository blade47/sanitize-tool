{
  "name": "@blade47/editorjs-sanitize-tool",
  "version": "1.0.0",
  "keywords": [
    "codex editor",
    "sanitize-tool",
    "editor.js",
    "editorjs"
  ],
  "description": "Sanitize Tool for Editor.js",
  "license": "MIT",
  "repository": "",
  "files": [
    "dist"
  ],
  "main": "./dist/sanitize-tool.umd.js",
  "module": "./dist/sanitize-tool.mjs",
  "exports": {
    ".": {
      "import": "./dist/sanitize-tool.mjs",
      "require": "./dist/sanitize-tool.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "yarn lint:fix && yarn prettier && vite build",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint --fix \"src/**/*.{js,jsx,ts,tsx}\"",
    "prettier": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\""
  },
  "author": {
    "name": "AFL Solutions SRL"
  },
  "devDependencies": {
    "vite": "^4.5.0",
    "vite-plugin-css-injected-by-js": "^3.3.0"
  },
  "dependencies": {
    "@codexteam/icons": "^0.0.5",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-perfectionist": "^2.11.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-unused-imports": "^4.0.0",
    "prettier": "^3.3.2"
  }
}
